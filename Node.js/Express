import fetch from "node-fetch";

async function openCloudAndroid(userId) {
  const res = await fetch("http://cloud-android-web:5000/api/emulator/url", {
    method: "POST",
    body: JSON.stringify({ userId }),
    headers: { "Content-Type": "application/json" }
  });
  const data = await res.json();
  return data;
}
